function plot_forces_AoA(path, cases, AoA, case_names)
    colors = [[0, 0.4470, 0.7410]; [0.8500, 0.3250, 0.0980]; ...
            [0.9290, 0.6940, 0.1250]; [0.4940, 0.1840, 0.5560]; ...
            [0.4660, 0.6740, 0.1880]; [0.3010, 0.7450, 0.9330]; ...
            [0.6350, 0.0780, 0.1840]; [0.25, 0.25, 0.25]];

    avg_forces = zeros(length(AoA), 6);
    std_forces = zeros(length(AoA), 6);
    
    for i = 1:length(AoA)
            load(path + cases(i) + '.mat');
            for j = 1:6
                avg_forces(i,j) = mean(filtered_data(:, j));
                std_forces(i,j) = std(filtered_data(:, j));
            end
    end
    
    x_label = "Angle of Attack (deg)";
    y_label_F = "Trial Average Force (N)";
    y_label_M = "Trial Average Moment (N*m)";
    axes_labels = [x_label, y_label_F, y_label_M];
    
    % Open a new figure.
    f = figure;
    f.Position = [200 50 900 560];
    tcl = tiledlayout(2,3);
    
    % Create three subplots to show the force time histories. 
    nexttile(tcl)
    scatter(AoA, avg_forces(:, 1));
    title(["F_x (streamwise)"]);
    xlabel(axes_labels(1));
    ylabel(axes_labels(2));
    
    nexttile(tcl)
    scatter(AoA, avg_forces(:, 2));
    title(["F_y (transverse)"]);
    xlabel(axes_labels(1));
    ylabel(axes_labels(2));
    
    nexttile(tcl)
    scatter(AoA, avg_forces(:, 3));
    title(["F_z (vertical)"]);
    xlabel(axes_labels(1));
    ylabel(axes_labels(2));
    
    % Create three subplots to show the moment time histories.
    nexttile(tcl)
    scatter(AoA, avg_forces(:, 4));
    title(["M_x (roll)"]);
    xlabel(axes_labels(1));
    ylabel(axes_labels(3));
    
    nexttile(tcl)
    scatter(AoA, avg_forces(:, 5));
    title(["M_y (pitch)"]);
    xlabel(axes_labels(1));
    ylabel(axes_labels(3));
    
    nexttile(tcl)
    scatter(AoA, avg_forces(:, 6));
    title(["M_z (yaw)"]);
    xlabel(axes_labels(1));
    ylabel(axes_labels(3));
    
    % Label the whole figure.
    sgtitle(["Force and Moment Means vs. Angle of Attack"]);

    %%
    clearvars -except AoA std_forces axes_labels

    x_label = "Angle of Attack (deg)";
    y_label_F = "Trial Standard Deviation Force (N)";
    y_label_M = "Trial Standard Deviation Moment (N*m)";
    axes_labels = [x_label, y_label_F, y_label_M];
    
    % Open a new figure.
    f = figure;
    f.Position = [200 50 900 560];
    tcl = tiledlayout(2,3);
    
    % Create three subplots to show the force time histories. 
    nexttile(tcl)
    for i = 1:length(AoA(:,1))
        scatter(AoA, std_forces(:, 1), DisplayName=case_names(i), Color=colors(i,:));
    end
    title(["F_x (streamwise)"]);
    xlabel(axes_labels(1));
    ylabel(axes_labels(2));
    
    nexttile(tcl)
    scatter(AoA, std_forces(:, 2));
    title(["F_y (transverse)"]);
    xlabel(axes_labels(1));
    ylabel(axes_labels(2));
    
    nexttile(tcl)
    scatter(AoA, std_forces(:, 3));
    title(["F_z (vertical)"]);
    xlabel(axes_labels(1));
    ylabel(axes_labels(2));
    
    % Create three subplots to show the moment time histories.
    nexttile(tcl)
    scatter(AoA, std_forces(:, 4));
    title(["M_x (roll)"]);
    xlabel(axes_labels(1));
    ylabel(axes_labels(3));
    
    nexttile(tcl)
    scatter(AoA, std_forces(:, 5));
    title(["M_y (pitch)"]);
    xlabel(axes_labels(1));
    ylabel(axes_labels(3));
    
    nexttile(tcl)
    scatter(AoA, std_forces(:, 6));
    title(["M_z (yaw)"]);
    xlabel(axes_labels(1));
    ylabel(axes_labels(3));
    
    hL = legend();
    hL.Layout.Tile = 'East';

    % Label the whole figure.
    sgtitle(["Force and Moment Standard Deviations vs. Angle of Attack"]);
end